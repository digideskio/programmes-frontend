{% extends base_template %}

{% block title %}Internal server error{% endblock %}

{% block body %}
    <div class="grid-wrapper">
        <div class="grid 2/3@bpb2 2/3@bpw">
            <h1>{{ block('title') }}</h1>
            <p>{% block pagenote %}Please try again later{% endblock %}</p>
        </div>
        <div class="grid 1/3@bpb2 1/3@bpw text--center">
            <img src="{% block image %}{{ asset('images/error/500.gif') }}{% endblock %}" alt="">
        </div>
    </div>
{% endblock %}
